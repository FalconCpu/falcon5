`timescale 1ns/1ns

module keyboard_if(
    input             clock,    // 100 Mhz clock
    input             reset,
    
	input    	      PS2_CLK2,
	input    	      PS2_DAT2,

    output reg[7:0]  keyboard_code,
    output reg       keyboard_strobe
);

initial begin
    keyboard_code = 8'bx;
    keyboard_strobe = 1'b0;
    @(posedge clock);
    @(posedge clock);
    @(posedge clock);
    @(posedge clock);
    keyboard_code = 8'h12;
    keyboard_strobe = 1'b1;
    @(posedge clock);
    keyboard_code = 8'hx;
    keyboard_strobe = 1'b0;
    @(posedge clock);
    @(posedge clock);
    @(posedge clock);
    @(posedge clock);
    keyboard_code = 8'h13;
    keyboard_strobe = 1'b1;
    @(posedge clock);
    keyboard_code = 8'hx;
    keyboard_strobe = 1'b0;
end

endmodule