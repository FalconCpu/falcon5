HWREGS_BASE = 0xE0000000
HWREGS_BLIT_CMD  = 0x0034   # Write commands here, read=# slots free
HWREGS_BLIT_CTRL = 0x0038

BLIT_CMD_SETUP = 0xFF000000
BLIT_CMD_RECT  = 0x01000000
BLIT_CMD_COPY  = 0x02000000
BLIT_CMD_TEXT  = 0x03000000

FONT_ADDR = 0x00010000  # where to put the font in SDRAM

# Configure the blitter registers
ld $1, HWREGS_BASE
ld $2, 1
stw $2, $1[HWREGS_BLIT_CTRL]  # Enable blitter privileged mode

ld $2, BLIT_CMD_SETUP
ld $3, 640
or $2, $3                     # $2 = SETUP + width
ld $3, 0x3f80000              # $3 = Screen base address
ld $4, 0x01E00280             # $4 = x2=640 y2=480
ld $5, FONT_ADDR              # $5 = font address in SDRAM
ld $6, 0x1008                 # $6 = font height=16, src width=8

stw $2, $1[HWREGS_BLIT_CMD]   # screen SETUP + width
stw $3, $1[HWREGS_BLIT_CMD]   # screen base addr
stw 0,  $1[HWREGS_BLIT_CMD]   # clip x1/y1
stw $4, $1[HWREGS_BLIT_CMD]   # clip x2/y2
stw 0,  $1[HWREGS_BLIT_CMD]   # offset x/y
stw $3, $1[HWREGS_BLIT_CMD]   # src addr
stw $4, $1[HWREGS_BLIT_CMD]   # src width
stw $5, $1[HWREGS_BLIT_CMD]   # font addr
stw $6, $1[HWREGS_BLIT_CMD]   # font width, src width

# copy the font into sdram
ld $1, font_start
ld $2, FONT_ADDR         # an arbitrary place in sdram
ld $3, font_end
copy_font:
ldw $4, $1[0]
stw $4, $2[0]
add $1, 4
add $2, 4
bne $1, $3, copy_font

# draw a black rectangle
ld $1, HWREGS_BASE
ld $5, BLIT_CMD_RECT
ld $6, 0x00100020
ld $7, 0x00200030
stw $5, $1[HWREGS_BLIT_CMD]    # DRAW_RECT color
stw $6, $1[HWREGS_BLIT_CMD]   # x1=16 y1=32
stw $7, $1[HWREGS_BLIT_CMD]   # x2=32 y2=48

ld $5, BLIT_CMD_TEXT
or $5, 15                      # color=4 (yellow)

ld $7, 0                       # char 'H'
ld $8, 256                     # end of font

# Loop through all the characters in the font
font_loop:
and $2, $7, 0xC0               # get top 2 bits of char
lsl $2, 14                     # Shift to make Y coordinate
and $3, $7, 0x3F               # get bottom 6 bits of char
lsl $3, 3                      # Shift to make X coordinate
or $2, $3                     # combine to get offset into font

stw $5, $1[HWREGS_BLIT_CMD]   # DRAW_TEXT color
stw $2, $1[HWREGS_BLIT_CMD]   # coordinate
stw $7, $1[HWREGS_BLIT_CMD]   # character
stw 0,  $1[HWREGS_BLIT_CMD]    # null terminator
add $7, 1
bne $7, $8, font_loop


# draw "Hello" in cyan at 128,128
ld $5, BLIT_CMD_TEXT
or $5, 0x00000006             # color=6 (cyan)
ld $2, 0x00800080             # x1=128 y1=128
stw $5, $1[HWREGS_BLIT_CMD]   # DRAW_TEXT color
stw $2, $1[HWREGS_BLIT_CMD]   # coordinate
ld $3, 'H'                    
stw $3, $1[HWREGS_BLIT_CMD]   
ld $3, 'e'                    
stw $3, $1[HWREGS_BLIT_CMD]   
ld $3, 'l'                    
stw $3, $1[HWREGS_BLIT_CMD]   
stw $3, $1[HWREGS_BLIT_CMD]   
ld $3, 'o'                    
stw $3, $1[HWREGS_BLIT_CMD]   
stw 0, $1[HWREGS_BLIT_CMD]   



# wait forever
forever:
jmp forever


