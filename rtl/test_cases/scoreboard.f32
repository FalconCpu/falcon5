# Base setup
ld   $1, 0x2000          # Base address
ld   $2, 0xDEADBEEF       # Some constant
stw  $2, $1[0]           # store to [0x2000]

# Dependent load chains
ldw  $3, $1[0]           # load word into $3
add  $4, $3, $2           # dependent on load -> must stall until $3 ready
ldb  $5, $1[0]           # load byte from same address
sub  $6, $5, $4           # dependent on $5 load -> must stall

# Overlapping load to same destination
ldw  $7, $1[0]           # first load to $7
ldw  $7, $1[0]           # second load to $7, overwrites first
add  $8, $7, $2           # dependent on second load -> should get correct value

# Independent instruction should proceed immediately
ld   $9, 0x1234
add  $10, $9, $1          # depends on $9 and $1 (both ready)

# Check scoreboard with mixed latency
ldw  $11, $1[0]           # load $11
mul  $12, $11, $2         # multiply dependent on $11 (long latency)
add  $13, $12, $3         # dependent on multiply

ret
